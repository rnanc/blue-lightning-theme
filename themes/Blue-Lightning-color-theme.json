	// Activity
	"activityBarBadge.background": "#ff2f2f",
	"activityBar.background": "#16161D",
	"activityBar.foreground": "#fff",
	"activityBar.border": "#00efff",
	"activityBar.inactiveForeground": "#8e8e8e",
	"activityBar.activeFocusBorder": "#00efff",
