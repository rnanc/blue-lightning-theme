	// Title Bar
	"titleBar.activeForeground": "#fff",
	"titleBar.activeBackground":"#16161D",
	"titleBar.inactiveForeground": "#8e8e8e",
	"titleBar.inactiveBackground": "#16161D",
	
	// Status
	"statusBar.background": "#16161D",
