  "statusBarItem.remoteBackground": "#16161D",
  "statusBarItem.remoteForeground": "#fff",

  // Notification
  "notifications.background": "#16161D",
  "notifications.foreground": "#fff",
	
	// Tab
	"tab.activeBackground": "#101010",
	"tab.activeForeground": "#fff",
	"tab.activeBorder": "#00efff",
	"tab.activeBorderTop": "#101010",
	"tab.inactiveForeground": "#8e8e8e",
	"tab.inactiveBackground": "#16161D",
	"tab.hoverForeground": "#00efff",
	
	// Debug
	"debugToolBar.background": "#101010",
	"debugView.stateLabelBackground": "#ff2f2f",
	
	
	// SideBar
	"sideBarTitle.foreground": "#F8F8F2",
	"sideBar.background": "#16161D",
	"sideBarSectionHeader.background": "#131316",
	"sideBarSectionHeader.foreground": "#fff", 
		
	// Window
	"window.activeBorder":"#16161D",
	
	// Terminal
	"terminal.background": "#16161D",
		
	// Badge
	"badge.background": "#ff2f2f",

	// Breadcrumb
	"breadcrumb.background": "#1a1a20",
	"breadcrumb.foreground": "#8e8e8e",
	"breadcrumb.focusForeground": "#00efff",
	"breadcrumbPicker.background": "#16161D",

	//Dropdown
	"dropdown.background": "#16161D",
	
